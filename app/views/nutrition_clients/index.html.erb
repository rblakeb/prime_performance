<p id="notice"><%= notice %></p>

<h1>Nutrition Clients</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nutrition_clients.each do |nutrition_client| %>
      <tr>
        <td><%= link_to 'Show', nutrition_client %></td>
        <td><%= link_to 'Edit', edit_nutrition_client_path(nutrition_client) %></td>
        <td><%= link_to 'Destroy', nutrition_client, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nutrition Client', new_nutrition_client_path %>
